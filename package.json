{
  "name": "angularexampleapp",
  "version": "19.1.0",
  "scripts": {
    "prepare": "npx husky",
    "start": "ng serve --configuration=development-en --open --no-hmr",
    "start:es": "ng serve --configuration=development-es --open",
    "extract": "ng extract-i18n --format=xlf --output-path=src/locale",
    "lint": "ng lint && npm run stylelint",
    "stylelint": "npx stylelint \"**/*.{css,scss}\"",
    "test": "ng test --code-coverage --no-watch",
    "test:watch": "ng test --code-coverage",
    "e2e": "npm run playwright:install && npm run playwright:test",
    "playwright:install": "npx playwright install && npx playwright install-deps",
    "playwright:test": "npx playwright test",
    "build": "ng build",
    "build:stats": "ng build --configuration=production-sourcemaps --stats-json"
  },
  "dependencies": {
    "@angular/animations": "20.3.7",
    "@angular/common": "20.3.7",
    "@angular/compiler": "20.3.7",
    "@angular/core": "20.3.7",
    "@angular/forms": "20.3.7",
    "@angular/localize": "20.3.7",
    "@angular/platform-browser": "20.3.7",
    "@angular/platform-browser-dynamic": "20.3.7",
    "@angular/platform-server": "20.3.7",
    "@angular/router": "20.3.7",
    "@angular/ssr": "20.3.7",
    "express": "4.18.2",
    "ngx-progressbar": "14.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/build": "20.3.7",
    "@angular/cli": "20.3.7",
    "@angular/compiler-cli": "20.3.7",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.23.0",
    "@ngrx/component": "19.1.0",
    "@playwright/test": "1.51.1",
    "@shoelace-style/shoelace": "2.20.1",
    "@types/express": "4.17.17",
    "@types/node": "18.18.0",
    "@types/validator": "13.12.3",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@typescript-eslint/types": "8.29.0",
    "@typescript-eslint/utils": "8.29.0",
    "@vitest/coverage-v8": "4.0.4",
    "angular-eslint": "19.3.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-unicorn": "58.0.0",
    "husky": "9.1.7",
    "jsdom": "27.0.1",
    "ng-extract-i18n-merge": "3.1.0",
    "normalize.css": "8.0.1",
    "playwright": "1.51.1",
    "postcss-scss": "4.0.9",
    "prettier": "3.5.3",
    "prettier-eslint": "16.3.0",
    "stylelint": "16.17.0",
    "stylelint-config-recess-order": "6.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "ts-loader": "9.5.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vitest": "4.0.4"
  },
  "private": true
}
