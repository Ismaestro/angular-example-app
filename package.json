    "ng-bullet": "1.0.3",
    "ng-packagr": "4.7.1",
    "node-sass": "4.11.0",
    "path": "0.12.7",
    "protractor": "5.4.2",
    "standard-version": "5.0.1",
    "sw-precache": "^5.2.1",
    "ts-helpers": "1.1.2",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.3",
    "tsickle": "0.34.3",
    "tslib": "1.9.3",
    "tslint": "5.13.1",
    "typescript": "3.1.3",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.2.3"
  }
}
